{
    "id": "full-stack",
    "realm": "full-stack",
    "enabled": true,
    "clients": [
      {
        "clientId": "angular-app",
        "rootUrl": "http://localhost:4200/",
        "enabled": true,
        "redirectUris": [
            "http://localhost:4200/*"
        ],
        "webOrigins": [
            "http://localhost:4200"
        ],
        "publicClient": true,
        "protocolMappers": [
          {
            "name": "express-server-audience",
            "protocol": "openid-connect",
            "protocolMapper": "oidc-audience-mapper",
            "consentRequired": false,
            "config": {
              "included.client.audience": "express-server",
              "id.token.claim": "false",
              "access.token.claim": "true"
            }
          }
        ]
      },
      {
        "clientId": "express-server",
        "enabled": true,
        "bearerOnly": true
      }
    ],  
    "users": [
      {
        "username": "cool-user",
        "enabled": true,
        "email": "cool-user@full-stack.com",
        "firstName": "Cool",
        "lastName": "User",
        "credentials": [
          {
            "type": "password",
            "value": "password"
          }
        ],
        "realmRoles": ["user"],
        "clientRoles": {
            "account": ["view-profile"]
        }
      }
    ],
    "roles": {
      "realm": [
        {
          "name": "user"
        },
        {
          "name": "default-roles-full-stack",
          "composite": true,
          "composites": {
            "realm": [
              "user"
            ],
            "client": {
              "account": [
                "view-profile"
              ]
            }
          }
        }
      ]
    }
  }